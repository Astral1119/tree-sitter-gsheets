{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "gsheets",
      "camelcase": "gSheets",
      "title": "Google Sheets",
      "scope": "source.gsheets",
      "file-types": [
        "gsheets",
        "gse",
        "gsf"
      ],
      "injection-regex": "^gsheets$",
      "class-name": "TreeSitterGsheets"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Google Sheets formula grammar for tree-sitter ",
    "authors": [
      {
        "name": "Astral Cafe",
        "email": "astral@astral.cafe",
        "url": "https://astral.cafe/"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-gsheets"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}